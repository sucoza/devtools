{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "production": [
      "default",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/jest.config.[jt]s",
      "!{projectRoot}/src/test-setup.[jt]s",
      "!{projectRoot}/test-setup.[jt]s",
      "!{projectRoot}/.eslintrc.json",
      "!{projectRoot}/eslint.config.js",
      "!{projectRoot}/**/*.stories.@(js|jsx|ts|tsx|mdx)",
      "!{projectRoot}/**/*.test.*",
      "!{projectRoot}/**/*.spec.*",
      "!{projectRoot}/example/**/*",
      "!{projectRoot}/coverage/**/*",
      "!{projectRoot}/.nx/**/*"
    ],
    "sharedGlobals": [
      "{workspaceRoot}/package.json",
      "{workspaceRoot}/pnpm-lock.yaml",
      "{workspaceRoot}/nx.json",
      "{workspaceRoot}/tsconfig.base.json",
      "{workspaceRoot}/.prettierrc",
      "{workspaceRoot}/.prettierignore",
      "{workspaceRoot}/.eslintrc.json",
      "{workspaceRoot}/.eslintignore",
      "{workspaceRoot}/pnpm-workspace.yaml"
    ]
  },
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"],
      "outputs": ["{projectRoot}/dist"],
      "metadata": {
        "description": "Build the project",
        "technologies": ["rollup"]
      }
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "test": {
      "cache": true,
      "inputs": ["default", "^production", "{workspaceRoot}/jest.preset.js"],
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "metadata": {
        "description": "Run unit tests",
        "technologies": ["vitest", "jest"]
      }
    },
    "lint": {
      "cache": true,
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json",
        "{workspaceRoot}/.eslintignore",
        "{workspaceRoot}/eslint.config.js"
      ],
      "outputs": ["{workspaceRoot}/dist/.eslintcache"],
      "metadata": {
        "description": "Lint the project",
        "technologies": ["eslint"]
      }
    },
    "typecheck": {
      "cache": true,
      "inputs": ["production", "^production"],
      "outputs": [],
      "metadata": {
        "description": "Type check the project",
        "technologies": ["typescript"]
      }
    },
    "format": {
      "cache": true,
      "inputs": [
        "default",
        "{workspaceRoot}/.prettierrc",
        "{workspaceRoot}/.prettierignore"
      ],
      "outputs": [],
      "metadata": {
        "description": "Format the project",
        "technologies": ["prettier"]
      }
    }
  },
  "plugins": [
    {
      "plugin": "@nx/rollup/plugin",
      "options": {
        "buildTargetName": "build",
        "serveTargetName": "dev"
      }
    }
  ],
  "generators": {
    "@nx/react": {
      "application": {
        "babel": true
      }
    }
  },
  "useInferencePlugins": false,
  "defaultBase": "main",
  "workspaceLayout": {
    "projectsDir": [
      "packages",
      "plugins"
    ]
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "nx.json": "*"
  },
  "pluginsConfig": {
    "@nx/rollup/plugin": {
      "buildTargetName": "build",
      "serveTargetName": "dev"
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "typecheck", "format"],
        "parallel": 4,
        "useDaemonProcess": true,
        "cacheDirectory": ".nx/cache",
        "accessToken": "${process.env.NX_CLOUD_ACCESS_TOKEN || ''}"
      }
    }
  },
  "nxCloudAccessToken": "${process.env.NX_CLOUD_ACCESS_TOKEN || ''}"
}