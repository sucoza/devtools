{
  "name": "@sucoza/router-devtools-plugin",
  "version": "1.0.0",
  "description": "Enhanced router debugging plugin for TanStack DevTools with live parameter editing and navigation timeline",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "devtools",
    "router",
    "react-router",
    "debugging",
    "tanstack",
    "react",
    "navigation"
  ],
  "author": "TanStack",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-router-dom": ">=6.0.0"
  },
  "dependencies": {
    "@tanstack/devtools-event-client": "^0.2.2",
    "@tanstack/react-devtools": "latest"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^0.34.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  }
}