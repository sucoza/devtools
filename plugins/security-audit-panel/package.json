{
  "name": "@sucoza/security-audit-panel-devtools-plugin",
  "version": "1.0.0",
  "description": "TanStack DevTools plugin for comprehensive security auditing including XSS, CSRF, CSP, dependency vulnerabilities, and more",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "example": "cd example && npm run dev",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "prepare": "npm run build"
  },
  "keywords": [
    "devtools",
    "security",
    "audit",
    "xss",
    "csrf",
    "csp",
    "vulnerability",
    "dependencies",
    "tls",
    "ssl",
    "owasp",
    "tanstack",
    "react"
  ],
  "author": "tyevco",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {    "@sucoza/devtools-common": "workspace:*",    "@sucoza/plugin-core": "workspace:*",    "@sucoza/shared-components": "workspace:*",
    "@tanstack/devtools": "^0.6.1",
    "@tanstack/devtools-event-client": "^0.2.2",
    "clsx": "^2.1.0",
    "lucide-react": "^0.263.1",
    "use-sync-external-store": "^1.2.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/use-sync-external-store": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.28.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
  }
}
