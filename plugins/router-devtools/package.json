{
    "name": "@sucoza/router-devtools-plugin",
    "version": "1.0.0",
    "description": "Enhanced router debugging plugin for TanStack DevTools with live parameter editing and navigation timeline",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "typecheck": "tsc --noEmit",
        "prepare": "npm run build"
    },
    "keywords": [
        "devtools",
        "router",
        "react-router",
        "debugging",
        "tanstack",
        "react",
        "navigation"
    ],
    "author": "tyevco",
    "license": "MIT",
    "peerDependencies": {
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0",
        "react-router-dom": ">=6.0.0"
    },
    "dependencies": {
        "@sucoza/devtools-common": "workspace:*",
        "@sucoza/plugin-core": "workspace:*",
        "@sucoza/shared-components": "workspace:*",
        "@tanstack/devtools-event-client": "^0.2.2",
        "@tanstack/react-devtools": "latest"
    },
    "devDependencies": {
        "@types/react": "^19.1.12",
        "@types/react-dom": "^19.1.9",
        "@vitejs/plugin-react": "^5.0.2",
        "typescript": "^5.9.2",
        "vite": "^7.1.3",
        "vitest": "^3.2.4",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-router-dom": "^7.8.2"
    }
}
