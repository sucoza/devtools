{
  "name": "@sucoza/browser-automation-test-recorder-devtools-plugin",
  "version": "1.0.0",
  "type": "module",
  "description": "DevTools plugin for recording browser interactions and generating test cases with smart selector generation and playback capabilities",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "example": "cd example && npm run dev",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run src/__tests__/unit",
    "test:integration": "vitest run src/__tests__/integration",
    "test:components": "vitest run src/__tests__/components",
    "test:performance": "vitest run src/__tests__/performance",
    "test:e2e": "playwright test --config src/__tests__/e2e/playwright.config.ts",
    "test:e2e:ui": "playwright test --config src/__tests__/e2e/playwright.config.ts --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage",
    "test:watch": "vitest",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:components && npm run test:performance",
    "test:ci": "npm run test:all && npm run test:e2e",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "format:check": "prettier --check src/**/*.{ts,tsx}",
    "analyze:bundle": "rollup -c --bundleConfigAsCjs --environment BUILD:production,ANALYZE:true",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test:ci && npm run build",
    "postinstall": "playwright install --with-deps"
  },
  "keywords": [
    "devtools",
    "browser-automation",
    "test-recorder",
    "playwright",
    "selenium",
    "test-generation",
    "selector-engine",
    "ui-testing",
    "e2e-testing",
    "tanstack",
    "react",
    "chrome-devtools-protocol"
  ],
  "author": "TanStack",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@sucoza/devtools-common": "workspace:*",
    "@sucoza/plugin-core": "workspace:*",
    "@sucoza/shared-components": "workspace:*",
    "@tanstack/devtools": "^0.6.1",
    "@tanstack/devtools-event-client": "^0.2.2",
    "@tanstack/react-devtools": "latest",
    "chrome-remote-interface": "^0.33.0",
    "playwright-core": "^1.40.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.263.1",
    "use-sync-external-store": "^1.2.0",
    "zustand": "^4.5.0",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@playwright/test": "^1.40.0",
    "@types/chrome-remote-interface": "^0.31.8",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/pixelmatch": "^5.2.4",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.4",
    "@vitest/ui": "^0.34.4",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-html-reporters": "^3.1.5",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.28.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "ts-jest": "^29.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
  }
}