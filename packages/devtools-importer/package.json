{
  "name": "@sucoza/devtools-importer",
  "version": "0.1.9",
  "description": "Simplified Vite plugin for importing and configuring TanStack DevTools plugins",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.js",
      "types": "./dist/react.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src/**/*.{ts,tsx}"
  },
  "keywords": [
    "devtools",
    "tanstack",
    "vite",
    "plugin",
    "react",
    "development"
  ],
  "author": "tyevco",
  "license": "MIT",
  "peerDependencies": {
    "@tanstack/devtools-vite": "0.2.10",
    "@tanstack/react-devtools": "0.5.5",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "vite": ">=5.0.0"
  },
  "dependencies": {
    "@tanstack/devtools": "catalog:",
    "@tanstack/devtools-event-bus": "catalog:",
    "@sucoza/logger": "workspace:*"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "catalog:",
    "@rollup/plugin-node-resolve": "catalog:",
    "@rollup/plugin-typescript": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-react": "catalog:",
    "eslint-plugin-react-hooks": "catalog:",
    "prettier": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "rollup": "catalog:",
    "rollup-plugin-peer-deps-external": "catalog:",
    "rollup-plugin-postcss": "catalog:",
    "tslib": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  }
}
