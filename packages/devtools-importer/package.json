{
  "name": "@sucoza/devtools-importer",
  "version": "0.1.0",
  "description": "Simplified Vite plugin for importing and configuring TanStack DevTools plugins",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.js",
      "types": "./dist/react.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src/**/*.{ts,tsx}"
  },
  "keywords": [
    "vite",
    "plugin",
    "tanstack",
    "devtools",
    "react",
    "development"
  ],
  "author": "tyevco",
  "license": "MIT",
  "peerDependencies": {
    "vite": ">=5.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "@tanstack/react-devtools": ">=1.0.0",
    "@tanstack/devtools-vite": ">=1.0.0"
  },
  "dependencies": {
    "@tanstack/devtools": "^0.6.1",
    "@tanstack/devtools-event-bus": "^0.6.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.28.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.8.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  }
}